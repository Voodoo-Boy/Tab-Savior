{
    "name": "Tab Savior",
    "description": "Save tabs for reopen next time",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "CopyLinkText/CopyLinkText-BackgroundScript.js"
        ],
        "persistent": false
    },
    "permissions": [
        "contextMenus",
        "activeTab",
        "tabs",
        "clipboardWrite"
    ],
    "icons": {
        "16": "Icon/TabSaviorIcon16x16.png",
        "48": "Icon/TabSaviorIcon48x48.png",
        "128": "Icon/TabSaviorIcon128x128.png"
    }
}