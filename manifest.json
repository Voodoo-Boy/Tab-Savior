{
    "name": "Tab Savior",
    "description": "Save tabs for reopen next time",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "BackgroundScript/BackgroundScript-ContextMenu-CopyLinkName.js"
        ],
        "persistent": false
    },
    "permissions": [
        "contextMenus"
    ]
}