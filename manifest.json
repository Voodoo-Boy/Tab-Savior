{
    "name": "Tab Savior",
    "description": "Save tabs for reopening next time",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "persistent": false,
        "scripts": [
            "background-contextmenu.js"
        ]
    }
}